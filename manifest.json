{
  "name": "sweccathon extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "main.html",
    "default_icon": "images/swecc_icon.png"
  }, 
  "content_scripts": [
    {
      "js": ["scripts/popup.js"],
      "matches": [
        "https://chatgpt.com/*"
      ]
    }
  ]
}